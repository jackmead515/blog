{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "d3": "^5.12.0",
    "highlight.js": "^10.4.1",
    "just-detect-adblock": "^1.0.0",
    "moment": "^2.24.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.1",
